{
    "name": "TEST TEST Oracle-XE-Apex Space",
    "workspaceFolder": "/app",
    "forwardPorts": [1521, 8181],
    "portsAttributes": {
        "1521": {
            "label": "oracle-free"
        },
        "8181": {
            "label": "ords-apex",
            "onAutoForward": "openPreview"
        }
    },
    "postCreateCommand": [
        "echo $ORACLE_DOCKER_REGISTRY_PASSWORD | docker login container-registry.oracle.com --username $ORACLE_DOCKER_REGISTRY_USERNAME --password-stdin","./setup_script.sh"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/sh"
    },
    "remoteEnv": {
        "ORACLE_PASSWORD": "${localEnv:ORACLE_PASSWORD}",
        "ORACLE_DOCKER_REGISTRY_USERNAME": "${localEnv:ORACLE_DOCKER_REGISTRY_USERNAME}",
        "ORACLE_DOCKER_REGISTRY_PASSWORD": "${localEnv:ORACLE_DOCKER_REGISTRY_PASSWORD}",
        "CONN_STRING": "${localEnv:CONN_STRING}"
    }
}

