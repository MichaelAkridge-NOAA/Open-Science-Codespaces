{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "hostRequirements": {
    "cpus": 4
  }, 
  "waitFor": "onCreateCommand",
  "updateContentCommand": "python3 -m pip install -r requirements.txt",
  "postCreateCommand": "",
  "postStartCommand": "chmod +x /workspaces/Open-Science-Codespaces/.devcontainer/test2/post_start.sh && /workspaces/Open-Science-Codespaces/.devcontainer/test2/post_start.sh",
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
        "Ikuyadeu.r"
      ]
    }
  }
}

