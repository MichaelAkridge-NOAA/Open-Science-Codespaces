{
  "name": "test2RStudio in Codespaces",
  "dockerComposeFile": "../../app/rstudio-test/docker-compose.yml",
  "service": "rstudio",
  "workspaceFolder": "/app",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "forwardPorts": [8787],
  "portsAttributes": {
    "8787": {
      "label": "rstudio",
      "onAutoForward": "openPreview"
    }
  },
"postStartCommand": "chmod +x /app/post_start.sh && /app/post_start.sh",
  "extensions": []
}

