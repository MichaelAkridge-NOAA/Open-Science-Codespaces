{
  "name": "Jupyter Book Dev Container",
  "dockerComposeFile": "docker-compose.yml",
  "service": "jupyterbook",
  "workspaceFolder": "/app",
  "settings": { 
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "forwardPorts": [4000],
  "portsAttributes": {
    "8501": {
      "label": "Jupyter Book",
      "onAutoForward": "openPreview"
    }
  }
}

